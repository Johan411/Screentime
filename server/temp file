create table login ( name varchar(20) , email varchar(40) primary key, password varchar(30) not null);

create table device (email varchar(40) , deviceid varchar(40) not null ,hash varchar(100) , FOREIGN KEY (email) REFERENCES login(email));

create table session( hash varchar(100) , type varchar(40) not null , start timestamp ,stop timestamp );


